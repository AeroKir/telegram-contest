{
  "name": "telegram-contest-app",
  "version": "1.0.0",
  "description": "App which show Followers statistic",
  "main": "index.js",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "syntax-async-functions",
      "transform-regenerator"
    ]
  },
  "scripts": {
    "test": "nyc mocha --require @babel/register",
    "watch": "webpack --watch",
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "server": "node server.js",
    "build": "webpack --config webpack.prod.js --mode production"
  },
  "keywords": [
    "Telegram",
    "contest",
    "Followers"
  ],
  "author": "Kirill Shevtsov",
  "license": "ISC",
  "devDependencies": {
